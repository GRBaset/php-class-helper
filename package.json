{
    "name": "php-class-helper",
    "displayName": "PHP Class Helper",
    "description": "Be more productive in writing php code with a simple keybinding.",
    "version": "0.0.4",
    "publisher": "predrag-nikolic",
    "homepage": "https://github.com/predragnikolic/php-class-helper",
    "repository": {
        "type": "git",
        "url": "https://github.com/predragnikolic/php-class-helper"
    },
    "icon": "resources/icon.png",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "php-class-helper.run",
                "key": "ctrl+alt+c",
                "when": "editorTextFocus && editorLangId == php"
            }
        ],
        "configuration": [
            {
                "title": "PHP Class helper Configuration options",
                "properties": {
                    "php-class-helper.visibility": {
                        "type": "string",
                        "default": "private",
                        "description": "Default visibility of a property"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}